    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            


            "args": ["--exp_name", "paper_AR_OT=5e-5", "--reg_OT_dir", "norm", "--lbd_OT", "5e-5",
                "--dataset_name", "hepmass", "--train_batch_size", "512", "--num_training_steps", "1",
                 "--learning_rate", "5e-4", "--num_flow_steps", "10", "--num_transform_blocks", "2",
                  "--hidden_features", "256", "--num_bins", "8", "--dropout_probability", "0.2",
                   "--base_transform_type", "rq-autoregressive", "--tabular_subset", "true"
            ],


            "cwd": "${fileDirname}",
            "env": {"PYDEVD_WARN_SLOW_RESOLVE_TIMEOUT": "2"}
        }
    ]
}